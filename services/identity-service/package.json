    "@prisma/client": "4.8.1",
    "crypto-ld": "3.9.0",
    "did-resolver": "^4.1.0",
    "encrypt-rsa": "^2.0.1",
    "hashi-vault-js": "^0.4.12",
    "jsonld": "^8.3.2",
    "jsonld-signatures": "6.0.0",
    "jwks-rsa": "^3.0.0",
